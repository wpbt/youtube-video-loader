{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "youtube-video-loader/ytvl-block",
    "version": "1.0.0",
    "title": "YouTube Video Loader",
    "description": "Insert YouTube videos in the page efficiently",
    "category": "media",
    "keywords": [ "yt", "youtube video loader" ],
    "supports": {
        "html": false
    },
    "attributes": {
        "embedUrl": {
            "type": "string"
        },
        "useCustomPreviewImage": {
            "type": "boolean"
        },
        "customPreviewImage": {
            "type": "array"
        },
        "ytThumb": {
            "type": "string"
        }
    },
    "textdomain": "youtube-video-loader",
    "style": "file:./index.css",
    "editorScript": "file:./index.js",
    "viewScript": "file:./ytvl-script.js"
}